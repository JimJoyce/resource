{
	"name": "resourcer",
	"manifest_version": 2,
	"description": "Find something interesting? Send it to your notes.",
	"version": "0.2",
	"icons": {"128": "re_icon.png"},
	"background": {
		"page": "background.html"
	},
	"browser_action": {
    "default_icon": "re_icon.png",
    "default_title": "resource"
  },
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["scripts/jquery-2.1.4.min.js" ,"script.js"]
		}
	],
	"permissions": [
		"tabs"
	]
}